{
    "name": "AWS EXTRA HEADER",
    "version": "1.0",
    "description": "A Chrome extension that adds an extra header bar on top of native navigation bar",
    "content_scripts": [
      {
        "matches": ["http://*.aws.amazon.com/*","https://*.aws.amazon.com/*"],
        "js": ["js/addAWSHeadder.js","libs/jdenticon-3.0.1/jdenticon-3.0.1.min.js"],
        "css" : ["css/cusomAWSHeadder.css"]
      }
    ],
    "permissions": [
      "storage"
    ],
    "options_page": "options.html",
    "icons": {
      "16": "icon/icon16.png",
      "48": "icon/icon48.png",
      "128": "icon/icon128.png"
    },
    "manifest_version": 2
}
